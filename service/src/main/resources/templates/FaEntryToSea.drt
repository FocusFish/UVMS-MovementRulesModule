package eu.europa.ec.fisheries.uvms.rules.service.business.activity;

import eu.europa.ec.fisheries.uvms.rules.service.business.fact.FaEntryToSeaFact;

template "@{templateName}"

rule "Vessel Transport means @{tname} - @{brid}"

when

    $fact : FaEntryToSeaFact(@{expression})

then
    $fact.setOk(false);
    $fact.addWarningOrError("@{type}", "@{rulemsg}", "@{brid}");

end

end template